{
    "name": "kocsismate/di-container-benchmarks",
    "description": "Benchmarks of some popular PHP DI Containers",
    "license": "MIT",
    "homepage": "https://github.com/kocsismate/php-di-benchmarks",
    "keywords": ["Dependency Injection Container benchmarks", "Dependency Injection Container performance", "DI"],
    "authors": [
        {
            "name": "Máté Kocsis",
            "email": "kocsismate@woohoolabs.com"
        },
        {
            "name": "Kristoffer Follestad",
            "email": "kristoffer@bonsy.no"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/container": "^2.0.2",
        "composer-runtime-api": "^2.0",
        "aura/di": "^5.0.8",
        "chubbyphp/chubbyphp-container": "^2.3.0",
        "doctrine/cache": "^2.2.0",
        "illuminate/container": "^11.37.0",
        "laminas/laminas-servicemanager": "^4.3.0",
        "level-2/dice": "^4.0.4",
        "php-di/php-di": "^7.0.7",
        "symfony/config": "^7.2",
        "symfony/dependency-injection": "^7.2.0",
        "woohoolabs/zen": "^3.1.1",
        "yiisoft/yii2": "^2.0.51",
        "rammewerk/container": "^1.1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.1",
        "phpstan/phpstan-strict-rules": "^2.0.1",
        "squizlabs/php_codesniffer": "^3.11.2",
        "woohoolabs/coding-standard": "^2.4.0"
    },
    "autoload": {
        "psr-4": {
            "DiContainerBenchmarks\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf"
    },
    "config": {
        "bin-compat": "full",
        "platform": {
            "php": "8.0.0"
        },
        "platform-check": false,
        "sort-packages": true,
        "allow-plugins": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
